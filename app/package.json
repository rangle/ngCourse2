    "selenium-standalone": "^4.7.1",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.1",
    "tslint-loader": "^1.0.2",
    "typescript": "^1.8.0",
    "typings": "^1.0.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.9"
  }
}
