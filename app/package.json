{
  "name": "ngCourse2",
  "version": "0.1.0",
  "description": "An AngularJS Course from Rangle.io",
  "license": "MIT",
  "keywords": [
    "angular2",
    "redux",
    "typescript"
  ],
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "postinstall": "npm run typings && npm run build",
    "typings": "rimraf typings && node_modules/.bin/typings install",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack",
    "test": "karma start",
    "e2e": "npm run build && gulp e2e",
    "toc": "doctoc --notitle ../handout && node toc.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rangle/ngCourse2.git"
  },
  "bugs": {
    "url": "https://github.com/rangle/ngCourse2.git/issues"
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.1",
    "@angular/compiler": "^2.0.0-rc.1",
    "@angular/core": "^2.0.0-rc.1",
    "@angular/http": "^2.0.0-rc.1",
    "@angular/platform-browser": "^2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.1",
    "@angular/router": "^2.0.0-rc.1",
    "@angular/router-deprecated": "^2.0.0-rc.1",
    "basscss": "^7.1.0",
    "basscss-border-colors": "^1.1.1",
    "chalk": "^1.1.1",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "express": "^4.13.3",
    "immutable": "^3.7.6",
    "ng2-redux": "^2.0.2",
    "react": "^0.14.5",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "redux-logger": "^2.1.4",
    "redux-thunk": "^2.1.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "^5.0.0-beta.6",
    "serve-webpack-client": "0.0.4",
    "winston": "^2.1.1",
    "zone.js": "^0.6.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "chai": "^3.4.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.21.0",
    "doctoc": "^0.15.0",
    "file-loader": "^0.8.4",
    "gulp": "^3.9.0",
    "gulp-webdriver": "^1.0.1",
    "gulp-webserver": "^0.9.1",
    "html-webpack-plugin": "^1.6.2",
    "istanbul": "^0.4.0",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.7",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "nodemon": "^1.8.1",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^7.1.3",
    "postcss-loader": "^0.8.0",
    "ramda": "^0.18.0",
    "raw-loader": "^0.5.1",
